public class KadaneAlgo {

	/**
	 * @param args
	 */

	static int findMaxSum(int arr[]) {
		int maxIndingHere =0; int maxSoFar = 0;
		
		for(int i =0; i< arr.length; i++) {
			maxIndingHere += arr[i];
			
		}
	}

	public static void main(String[] args) {
		int arr[] = { 2, -4, 5, 6, -11 };
	}

